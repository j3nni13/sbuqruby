    <div class="row">
        <div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper" class="nav-collapse ">
            <ul class="sidebar-nav">
                <%= render 'pages/dashsidebar' %>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h1>My Listings</h1>
                        <% if can? :manage, Listing %>
                 <%= link_to "New Listing", new_listing_path %>
             

         <table class="table" style="width:100%">
        <thead>
          <tr>
            <th>Name</th>
            <th>Price</th>  
            <th></th>
            <th></th>
             <th></th>
          </tr>
        </thead>
        <tbody>
        <% @listings.each do |listing| %> 
          <tr>
            <td><%= link_to listing.title, edit_listing_path(listing) %></td>
            <td><%= listing.price %></td>
            <td><%= link_to "Edit", edit_listing_path(listing) %></td>
            <td><%= link_to "View", listing_path(listing) %></td>   
            <td style="width: 20%"> <%= link_to image_tag(listing.pictures.first.photo.url, :width => '200px'), edit_listing_path(listing) %></td>
          </tr>
        <% end %>
        </tbody>
      </table>

      <% else %>
      Sorry, you are not a venue manager. Please contact us to become one.
      <br>
      <br>
       <%= link_to "Request to be a Venue Manager", new_conversation_path(to: @user_id = "1"), class: "btn btn-large btn-primary" %>
        <% end %> 

    

                        
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    </script>

